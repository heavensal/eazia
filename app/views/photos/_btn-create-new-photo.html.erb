<div class="generator-image">
  <%= form_with model: post, url: post_photos_path(post), method: :post, local: true, data: { action: "submit->form-submission#submitImage" } do |form| %>
  <div data-form-submission-target="images">
  </div>
    <%= form.submit "Générer une image", class:"secondary-bouton"%>
  <% end %>
</div>
